<head>
	<title>Dashboard</title>
</head>
<div id="wrapper" class='w-auto'>
	<!-- SideBar -->
	<%- include('./dashboardNavSide.ejs') %>
	<!-- End of Sidebar -->
	<!-- Content Wrapper -->
	<div id="content-wrapper" class="d-flex flex-column">
		<!-- Main Content -->
		<div id="content">
			<!-- Main Content -->
			<div id="content">
				<!-- Topbar -->
				<%- include('./partials/Logout.ejs') %>
				<!-- End of Topbar -->
				<!-- Begin Page Content -->
				<div class="container-fluid">
					<!-- Page Heading -->
					<div class="row d-flex justify-content-around mb-1">
						<!-- Users Registered List Card Example -->
						<div class="col-xl-4 col-md-3 mb-1">
							<div
								class="card border-left-primary shadow h-100 w-auto py-1">
								<div class="card-body">
									<div
										class="row no-gutters align-items-center">
										<div class="col mr-2">
											<div
												class="text-xs font-weight-bold text-primary text-uppercase mb-1">
												Users Registered</div>
											<div
												class="h5 mb-0 font-weight-bold text-gray-800">
												<%=userCount %></div>
										</div>
										<div class="col-auto">
											<i class="fas fa-users fa-2x"></i>
										</div>
									</div>
								</div>
							</div>
						</div>
						<!-- Surveys Total Card Example -->
						<div class="col-xl-4 col-md-3 mb-1">
							<div
								class="card border-left-success shadow h-100 w-auto py-1">
								<div class="card-body">
									<div
										class="row no-gutters align-items-center">
										<div class="col mr-2">
											<div
												class="text-xs font-weight-bold text-success text-uppercase mb-1">
												Surveys</div>
											<div
												class="h5 mb-0 font-weight-bold text-gray-800">
												<%= surveys %></div>
										</div>
										<div class="col-auto">
											<i
												class="fas fa-question-circle fa-2x"></i>
										</div>
									</div>
								</div>
							</div>
						</div>
						<div class="col-xl-4 col-md-3 mb-1">
							<div
								class="card border-left-success shadow h-100 w-auto py-1">
								<div class="card-body">
									<div
										class="row no-gutters align-items-center">
										<div class="col mr-2">
											<div
												class="text-xs font-weight-bold text-success text-uppercase mb-1">
												Users surveys Performed</div>
											<div
												class="h5 mb-0 font-weight-bold text-gray-800">
												<%= performedSurveyCount %>
											</div>
										</div>
										<div class="col-auto">
											<i
												class="fas fa-question-circle fa-2x"></i>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
					<div class="card shadow mb-1">
						<div class="card-header py-1">
							<h6 class="m-0 font-weight-bold text-primary">
								Surveys Performed List</h6>
						</div>
						<div class="card-body">
							<div class="table-responsive w-100">
								<table class="table table-bordered"
									id="dataTable" width="100%" cellspacing="0">
									<thead>
										<tr>
											<th class="w-25 text-nowrap">Survey	Title</th>
											<th class="w-100 text-nowrap">Description</th>
											<th class="w-25 text-nowrap">Users</th>
											<th class='w-25 text-nowrap'>Assign Form</th>
										</tr>
									</thead>
									<tbody>
										<% for(var performedSurvey of performedSurvey){ %>
										<% if(performedSurvey) {%>
										<tr>
											<td class="test-nowrap">
												<%= performedSurvey.surveyId.title %>
											</td>
											<td><%= performedSurvey.surveyId.description %>
											</td>
											<td>
											<div class='d-flex justify-content-center'>
												<a href="dashboard/user_surveys/<%= performedSurvey._id %>"
													class="badge badge-success text-white">Users</a>
											</div>
											</td>
											<td>
											<div class='d-flex justify-content-center'>
												<a href="/admin/assign-form/<%= performedSurvey.surveyId.category %>"
													class="badge badge-info text-white">Forms</a>
											</div>
											</td>
										</tr>
										<% } %> <% } %>
									</tbody>
								</table>
							</div>
						</div>
					</div>
					<!-- /.container-fluid -->
				</div>
				<!-- End of Content Wrapper -->
			</div>
		</div>
		<%- include('./partials/footer.ejs') %>
		<a class="scroll-to-top rounded" href="#page-top">
	<i class="fas fa-angle-up">UP</i>
</a>
	</div>
</div>

<!-- Scroll to Top Button-->
<a class="scroll-to-top rounded" href="#page-top">
	<i class="fas fa-angle-up">UP</i>
</a>

<!-- Logout Modal-->
<%- include('./partials/logoutModel.ejs') %>